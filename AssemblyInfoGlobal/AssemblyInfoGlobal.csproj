REM Get Revision
cd "$(SolutionDir)"
git  rev-list HEAD --count&gt;"$(SolutionDir)revision"
set /p revision=&lt;"$(SolutionDir)revision"

REM Initial version information
set VERSIONFILE=$(SolutionDir)version.properties
for /f "usebackq eol=# delims=" %25%25x in ("%25VERSIONFILE%25") do (
      set "%25%25x"
)

if "%25version.number%25"=="" echo VERSION is empty, aborting build&amp;&amp;exit 1
echo Embed version number in  "$(ProjectDir)Properties\AssemblyInfo.Global.cs"
set version=%25version.number%25.%25revision%25
echo %25version%25&gt; $(TargetDir)version
"$(SolutionDir)_BuildUtils\ReplaceAssemblyVersion.exe" -set:%25version%25 "$(ProjectDir)Properties\AssemblyInfo.Global.cs"</PreBuildEvent>
